{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strict": false
  },
  "include": ["src/**/*"],
  "exclude": ["src/**/*.test.*", "src/**/__tests__/**"]
}
